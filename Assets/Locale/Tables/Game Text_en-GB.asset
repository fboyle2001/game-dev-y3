    m_Metadata:
      m_Items: []
  - m_Id: 33079581343744
    m_Localized: Level Up!
    m_Metadata:
      m_Items: []
  - m_Id: 33526174056448
    m_Localized: Press [E] or [Button West]
    m_Metadata:
      m_Items: []
  - m_Id: 33994317103104
    m_Localized: Free {global.cat_name}
    m_Metadata:
      m_Items: []
  - m_Id: 34086843449344
    m_Localized: Time to go free {global.cat_name}! I can hear them just inside the
      cave.
    m_Metadata:
      m_Items:
      - id: 0
